#!/bin/sh

# Add RVM to PATH for scripting
PATH="${PATH}:$HOME/.rvm/bin"

# Setting PATH for Python 3.4
PATH="${PATH}:/Library/Frameworks/Python.framework/Versions/3.4/bin"

# Setting PATH for Python 3.5
PATH="${PATH}:/Library/Frameworks/Python.framework/Versions/3.5/bin"

PATH="${PATH}:/Applications/Postgres.app/Contents/Versions/latest/bin"

PATH="${PATH}:/usr/local/opt/coreutils/libexec/gnubin"

# Mera laptop hai tujhe kya?
PATH="${PATH}:/usr/local/sbin"

# Executables
PATH="${PATH}:~/workplace/executables"

PATH="${PATH}:/usr/local/Cellar/php70/7.0.12_5/bin"

# Go
export GOPATH="${HOME}/workplace/go"
PATH="${PATH}:~/workplace/go/bin"
PATH="${PATH}:/usr/local/opt/go/libexec/bin"

export PATH
